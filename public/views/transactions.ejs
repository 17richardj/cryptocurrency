    <main role="main" class="container" style="width: 100%">
      <div class="d-flex align-items-center p-3 my-3 text-white-50 bg-dark rounded box-shadow">
        <h1 class="mr-3" style="color: white">OC</h1><!--<img class="mr-3" src="https://images-platform.99static.com/S2Jnw62fQWtPYn9LGC3yJy1izNs=/500x500/top/smart/99designs-contests-attachments/29/29553/attachment_29553655" alt="" width="48" height="48">-->
        <div class="lh-100">
          <h6 class="mb-0 text-white lh-100">Email Blocks</h6>
          <small>Since 2020</small>
        </div>
      </div>
      <% if (typeof blocks !== 'undefined' && blocks.length > 0) { %>
      <div class="my-3 p-3 shadow-sm p-3 mb-5 bg-white rounded">
        <h6 class="border-bottom border-gray pb-2 mb-0">Recent transactions</h6>
        <div class="media text-muted pt-3">
          <img data-src="holder.js/32x32?theme=thumb&bg=007bff&fg=007bff&size=1" alt="" class="mr-2 rounded">
          <p class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray">
            <div class="container">
            <div class="row border-bottom border-gray">
              <div class="col-sm-8">
            <h6 class=" pb-2 mb-0">Hash</h6>
          </div>
          <div class="col-sm-2">
            <h6 class="pb-2 mb-0">Time</h6>
      </div>
      <div class="col-sm-2">
            <h6 class="pb-2 mb-0">Value</h6>
  </div>
</div>
</div>
          </p>
        </div>
        <% for(var i=0; i < blocks.length; i++) { %>
          <% for(var j=0; j < blocks[i].data.transaction_Pool.length; j++) { %>
        <div class="media text-muted pt-3">
          <img data-src="holder.js/32x32?theme=thumb&bg=007bff&fg=007bff&size=1" alt="" class="mr-2 rounded">
          <p class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray">
            <div class="container">
            <div class="row border-bottom border-gray">
              <div class="col-sm-8">
            <span class="d-block text-gray-dark"><%= blocks[i].data.transaction_Pool[j].input.txid %></span>
          </div>
          <div class="col-sm-2">
<% var date = new Date(blocks[i].data.transaction_Pool[j].lock_time * 1000); %>

<% var hours = date.getHours(); %>

<% var minutes = "0" + date.getMinutes(); %>

<% var seconds = "0" + date.getSeconds(); %>

<% var formattedTime = hours + ':' + minutes.substr(-2) + ':' + seconds.substr(-2); %>
        <%= formattedTime %>
      </div>
      <div class="col-sm-2">
      <%  if(blocks[i].data.transaction_Pool[j].output.amount == 0){ %>
        <span class="text-secondary" style="color: ;">spent</span>
      <% } else{ %>
    <span style="color: green;"><%= blocks[i].data.transaction_Pool[j].output.amount.toFixed(4) %> ODC</span>
    <% } %>
  </div>
</div>
</div>
          </p>
        </div>
        <% } %>
      <% } %>
        <small class="d-block text-right mt-3">
          <a href="#">All updates</a>
        </small>
      </div>

      <div class="my-3 p-3 shadow-sm p-3 mb-5 bg-white rounded" style="margin-bottom: 5%;">
        <h6 class="border-bottom border-gray pb-2 mb-0">Suggestions</h6>
        <% for(var i=0; i < blocks.length; i++) { %>
        <div class="media text-muted pt-3">
          <img data-src="holder.js/32x32?theme=thumb&bg=007bff&fg=007bff&size=1" alt="" class="mr-2 rounded">
          <div class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray">
            <div class="d-flex justify-content-between align-items-center w-100">
              <strong class="text-gray-dark">Block: <%= blocks[i].index %></strong>
              <a href="#">Follow</a>
            </div>
            <span class="d-block">@<%= blocks[i].hash %></span>
          </div>
        </div>
      <% } %>
        <small class="d-block text-right mt-3">
          <a href="#">All suggestions</a>
        </small>
      </div>
    </main>
		<% } else { %>

      <% } %>

